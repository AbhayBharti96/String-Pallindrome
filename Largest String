import java.util.*;
class Main{
    public static void main(String []args){
        String s[] = {"Apple","Mango","Guava","Banana","PineApple"};
        String largest = s[0];
        for(int i=1;i<s.length;i++){
            if(largest.compareTo(s[i])<0){
                largest= s[i];
            }
        }
        System.out.println("Largest String is: "+largest);
    }
}
